<% feedback ||= nil %>
<% progress ||= nil %>

<div id="report-status">
    <% case report.status %>
    <% when "pending" %>
    <%= render "reports/status/pending", report: report %>
    <% when "processing" %>
    <%= render "reports/status/processing", report: report, feedback: feedback, progress: progress %>
    <% when "completed" %>
    <%= render "reports/status/completed", report: report, feedback: feedback %>
    <% when "failed" %>
    <%= render "reports/status/failed", report: report %>
    <% end %>
</div>